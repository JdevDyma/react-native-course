{
  "expo": {
    "name": "project-shoes",
    "slug": "project-shoes",
    "scheme": "project-shoes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jdev99.ios",
      "buildNumber": "5"
    },
    "android": {
      
      "versionCode": 8,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jdev9999.projectshoes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-notifications",
      "react-native-iap"
    ],
    "extra": {
      "eas": {
        "projectId": "9490828b-8d46-41b2-99da-d4dee6bfa1ad"
      }
    },
    "owner": "jdev9999"
  }
}
