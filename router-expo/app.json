{
  "expo": {
    "name": "router-expo",
    "slug": "router-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-screen.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "router-expo",
    "splash": {
      "image": "./assets/splash-screen.png",
      "resizeMode": "cover",
      "enableFullScreenImage_legacy": true,
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
